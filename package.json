          "command": "commands-manager-next.time.startTimer",
          "when": "view == timeTrackerTree && viewItem == timerFolder && viewItem != timerArchiveFolder && viewItem != timerGitBranchesFolder",
          "group": "inline@1"
        },
        {
          "command": "commands-manager-next.time.startTimer",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped) && viewItem != 'timerRunning timerHasSubtimers' && viewItem != 'timerStopped timerHasSubtimers' && viewItem != 'timerRunning timerArchived timerHasSubtimers' && viewItem != 'timerStopped timerArchived timerHasSubtimers'",
          "group": "inline@1"
        },
        {
          "command": "commands-manager-next.time.resumeTimer",
          "when": "view == timeTrackerTree && (viewItem == 'timerStopped timerHasSubtimers' || viewItem == 'timerStopped timerArchived timerHasSubtimers')",
          "group": "inline@1"
        },
        {
          "command": "commands-manager-next.time.createSubTimer",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped || viewItem == 'timerRunning timerHasSubtimers' || viewItem == 'timerStopped timerHasSubtimers' || viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived' || viewItem == 'timerRunning timerArchived timerHasSubtimers' || viewItem == 'timerStopped timerArchived timerHasSubtimers')",
          "group": "inline@4"
        },
        {
          "command": "commands-manager-next.time.archiveTimer",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped || viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived')",
          "group": "1_actions@3"
        },
        {
          "command": "commands-manager-next.time.deleteTimer",
          "when": "view == timeTrackerTree && (viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived')",
          "group": "1_actions@4"
        },
        {
          "command": "commands-manager-next.time.moveTimerUp",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped || viewItem == 'timerRunning timerHasSubtimers' || viewItem == 'timerStopped timerHasSubtimers' || viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived' || viewItem == 'timerRunning timerArchived timerHasSubtimers' || viewItem == 'timerStopped timerArchived timerHasSubtimers')",
          "group": "2_modify@1"
        },
        {
          "command": "commands-manager-next.time.moveTimerDown",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped || viewItem == 'timerRunning timerHasSubtimers' || viewItem == 'timerStopped timerHasSubtimers' || viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived' || viewItem == 'timerRunning timerArchived timerHasSubtimers' || viewItem == 'timerStopped timerArchived timerHasSubtimers')",
          "group": "2_modify@2"
        },
        {
          "command": "commands-manager-next.time.moveToFolder",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped || viewItem == 'timerRunning timerHasSubtimers' || viewItem == 'timerStopped timerHasSubtimers' || viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived' || viewItem == 'timerRunning timerArchived timerHasSubtimers' || viewItem == 'timerStopped timerArchived timerHasSubtimers')",
          "group": "2_modify@3"
        },
        {
          "command": "commands-manager-next.time.createSubTimer",
          "when": "view == timeTrackerTree && (viewItem == timerRunning || viewItem == timerStopped || viewItem == 'timerRunning timerHasSubtimers' || viewItem == 'timerStopped timerHasSubtimers' || viewItem == 'timerRunning timerArchived' || viewItem == 'timerStopped timerArchived' || viewItem == 'timerRunning timerArchived timerHasSubtimers' || viewItem == 'timerStopped timerArchived timerHasSubtimers')",
          "group": "3_new@1"
        },
        {
          "command": "commands-manager-next.time.stopSubTimer",
          "when": "view == timeTrackerTree && viewItem == subtimerRunning",
          "group": "inline@1"
        },
        {
          "command": "commands-manager-next.time.editSubTimer",
          "when": "view == timeTrackerTree && (viewItem == subtimerRunning || viewItem == subtimerStopped)",
          "group": "inline@2"
        },
        {
          "command": "commands-manager-next.time.startSubTimer",
          "when": "view == timeTrackerTree && viewItem == subtimerStopped",
          "group": "inline@1"
        },
        {
          "command": "commands-manager-next.time.deleteSubTimer",
          "when": "view == timeTrackerTree && (viewItem == subtimerRunning || viewItem == subtimerStopped)",
          "group": "1_actions@2"
        },
        {
          "command": "commands-manager-next.time.newFolder",
          "when": "view == timeTrackerTree && viewItem == timerFolder && viewItem != timerArchiveFolder && viewItem != timerGitBranchesFolder",
          "group": "3_new@2"
        }
      ]
    },
    "configuration": {
      "title": "Commands Mnager Next",
      "properties": {
        "commands-manager-next.tasks.storageLocation": {
          "type": "string",
          "enum": [
            "workspace",
            "global",
            "both"
          ],
          "default": "workspace",
          "description": "Choose where to save commands: 'workspace' saves to .vscode/commands in the current workspace, 'global' saves to a global directory (~/.vscode/commands), 'both' uses both locations."
        },
        "commands-manager-next.tasks.preferGlobalCommands": {
          "type": "boolean",
          "default": false,
          "description": "When enabled, global commands from ~/.vscode/commands are loaded as default commands instead of the built-in samples. Only applies when storage location is set to 'both'."
        },
        "commands-manager-next.tasks.autoCreateCommandsDirectory": {
          "type": "boolean",
          "default": true,
          "description": "Automatically create the commands directory when opening a workspace. When disabled, the directory is only created when you explicitly create a command."
        },
        "commands-manager-next.tasks.addCommandsToGitignore": {
          "type": "boolean",
          "default": false,
          "description": "Automatically add the commands directory to .gitignore if the workspace is a Git repository. Helps keep local commands private."
        },
        "commands-manager-next.tasks.documentationHub.viewMode": {
          "type": "string",
          "enum": [
            "tree",
            "flat"
          ],
          "default": "tree",
          "description": "Choose whether the documentation hub displays markdown files in a folder tree or a flat list."
        },
        "commands-manager-next.tasks.documentationHub.position": {
          "type": "string",
          "enum": [
            "top",
            "bottom"
          ],
          "default": "bottom",
          "description": "Control whether the documentation hub appears above or below the command list in the Task and Documentation Hub view container."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node scripts/compile.js",
    "watch": "npm run compile && tsc -watch -p ./",
    "watch:all": "concurrently \"tsc -watch -p ./\" \"npm run watch:html\"",
    "watch:html": "node -e \"const fs=require('fs');const path=require('path');const {exec}=require('child_process');const watchDir='resources';let timeout;function compile(){clearTimeout(timeout);timeout=setTimeout(()=>exec('npm run compile',(e,o,er)=>console.log('HTML changed, compiling...')),100);}fs.watch(watchDir,{recursive:true},(e,f)=>f.endsWith('.html')&&compile());console.log('Watching HTML files in',watchDir);\"",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "test:watch": "npm run watch && npm run test",
    "test:playwright": "npm run compile && npx playwright test",
    "test:ui": "npx wdio run wdio.conf.js",
    "test:ui:single": "npx wdio run wdio.conf.js --spec",
    "test:all": "npm test && npm run test:playwright && npm run test:ui",
    "test:vet": "vscode-extension-tester --config ./et-tests/mocha-vscode.json"
  },
